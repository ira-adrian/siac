{% extends 'base.html.twig' %}

{% block body %}
    <h1>itemSolicitados list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Fecha</th>
                <th>Trimestre</th>
                <th>Codigo</th>
                <th>Item</th>
                <th>Condetalle</th>
                <th>Unidadmedida</th>
                <th>Precio</th>
                <th>Cantidad</th>
                <th>Estado</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for itemSolicitado in itemSolicitados %}
            <tr>
                <td><a href="{{ path('itemSolicitado_show', { 'id': itemSolicitado.id }) }}">{{ itemSolicitado.id }}</a></td>
                <td>{% if itemSolicitado.fecha %}{{ itemSolicitado.fecha|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ itemSolicitado.trimestre }}</td>
                <td>{{ itemSolicitado.codigo }}</td>
                <td>{{ itemSolicitado.item }}</td>
                <td>{{ itemSolicitado.conDetalle }}</td>
                <td>{{ itemSolicitado.unidadMedida }}</td>
                <td>{{ itemSolicitado.precio }}</td>
                <td>{{ itemSolicitado.cantidad }}</td>
                <td>{% if itemSolicitado.estado %}Yes{% else %}No{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('itemSolicitado_show', { 'id': itemSolicitado.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('itemSolicitado_edit', { 'id': itemSolicitado.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('itemSolicitado_new') }}">Create a new itemSolicitado</a>
        </li>
    </ul>
{% endblock %}
